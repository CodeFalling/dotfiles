set tabnumbers
set toolbars+=nonavigation

let mapleader = ","

nmap s :open duckduckgo 
nmap S :tabopen duckduckgo 

cmap <C-g> <Esc>

command pintab js getBrowser().pinTab(getBrowser().mCurrentTab);
map <Leader>p :pintab<CR>

source! ~/.vimperatorrc.local

